{
  "manifest_version": 3,
  "name": "Auto History Cleaner",
  "version": "2.0",
  "description": "Removes history of specific websites after closing their tabs.",
  "permissions": ["history", "tabs", "storage", "alarms"],
  "background": { "service_worker": "background.js" },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon.png"
    }
  },
  "icons": {
    "128": "icon.png"
  }
}
